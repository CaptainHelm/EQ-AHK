!^h::
{
	Gui, Color, CADD22
	Gui, +ToolWindow -Caption +E0x08000000
	Gui, Show, x1700 y1025 h85 w485, Everquest Alt Hot Bar
	Gui, +LastFound
	WinSet, TransColor, CADD22
	WinSet, AlwaysOnTop, On
	return
}

