#SingleInstance force
#UseHook
#NoEnv  ; Recommended for performance and compatibility with future AutoHotkey releases.
;#MaxThreadsPerHotkey
;#Warn  ; Enable warnings to assist with detecting common errors.
SendMode Input  ; Recommended for new scripts due to its superior speed and reliability.
SetWorkingDir %A_ScriptDir%  ; Ensures a consistent starting directory.


setTimer, eq, 1
#persistent




IniFile=Config.ini
IfExist, %IniFile%
{
	Movement:
	{
		Iniread, Move_Forward, Config.ini, Movement, Forward
		Iniread, Move_Backward, Config.ini, Movement, Backward
		Iniread, Move_Left, Config.ini, Movement, Left
		Iniread, Move_Right, Config.ini, Movement, Right
		Iniread, Character_sit, Config.ini, Movement, Sit
		Iniread, Character_Follow, Config.ini, Movement, Follow
	}
	
	Target_Party:
	{
		Iniread, Target_party_slot_1, Config.ini, Target Party Slot, Party Slot 1
		Iniread, Target_party_slot_2, Config.ini, Target Party Slot, Party Slot 2
		Iniread, Target_party_slot_3, Config.ini, Target Party Slot, Party Slot 3
		Iniread, Target_party_slot_4, Config.ini, Target Party Slot, Party Slot 4
		Iniread, Target_party_slot_5, Config.ini, Target Party Slot, Party Slot 5
		Iniread, Target_party_slot_6, Config.ini, Target Party Slot, Party Slot 6
		Iniread, Target_party_slot_self, Config.ini, Target Party Slot, Party Slot self
	}
	
	Hotbar_Row_1:
	{
		Iniread, Hotbar_row_1_slot_1, Config.ini, Hotbar Row 1, slot 1
		Iniread, Hotbar_row_1_slot_2, Config.ini, Hotbar Row 1, slot 2
		Iniread, Hotbar_row_1_slot_3, Config.ini, Hotbar Row 1, slot 3
		Iniread, Hotbar_row_1_slot_4, Config.ini, Hotbar Row 1, slot 4#SingleInstance force
#UseHook
#NoEnv  ; Recommended for performance and compatibility with future AutoHotkey releases.
;#MaxThreadsPerHotkey
;#Warn  ; Enable Warnings To Assist With Detecting Common Errors.
SendMode Input  ; Recommended for new scripts due to its superior speed and reliability.
SetWorkingDir %A_ScriptDir%  ; Ensures a consistent starting directory.


TransparentBar := 2

Checkboxs := Object("CB_State", "Check Box State", "T_CB", "Toggle Mimic")

Hotbar :="Everquest Alt Hot Bar"







setTimer, eq, 1
#persistent




IniFile=Config.ini
IfExist, %IniFile%
{
	
	Movement:
	{
		
		Iniread, Move_Forward, Config.ini, Movement, Forward ;reads ini config file, locates[Movement] section, finds Forward key & saves the hotkey to the Move_Forward Variable
		Iniread, Move_Backward, Config.ini, Movement, Backward
		Iniread, Move_Left, Config.ini, Movement, Left
		Iniread, Move_Right, Config.ini, Movement, Right
		Iniread, Character_sit, Config.ini, Movement, Sit
		Iniread, Character_Follow, Config.ini, Movement, Follow
	}
	
	Target_Party:
	{
		Iniread, Target_party_slot_1, %IniFile%, Target Party Slot, Party Slot 1 
		Iniread, Target_party_slot_2, %IniFile%, Target Party Slot, Party Slot 2
		Iniread, Target_party_slot_3, %IniFile%, Target Party Slot, Party Slot 3
		Iniread, Target_party_slot_4, %IniFile%, Target Party Slot, Party Slot 4
		Iniread, Target_party_slot_5, %IniFile%, Target Party Slot, Party Slot 5
		Iniread, Target_party_slot_6, %IniFile%, Target Party Slot, Party Slot 6
		Iniread, Target_party_slot_self, %IniFile%, Target Party Slot, Party Slot self
	}
	
	Hotbar_Row_1:
	{
		Iniread, Hotbar_row_1_slot_1, %IniFile%, Hotbar Row 1, slot 1
		Iniread, Hotbar_row_1_slot_2, %IniFile%, Hotbar Row 1, slot 2
		Iniread, Hotbar_row_1_slot_3, %IniFile%, Hotbar Row 1, slot 3
		Iniread, Hotbar_row_1_slot_4, %IniFile%, Hotbar Row 1, slot 4
		Iniread, Hotbar_row_1_slot_5, %IniFile%, Hotbar Row 1, slot 5
		Iniread, Hotbar_row_1_slot_6, %IniFile%, Hotbar Row 1, slot 6
		Iniread, Hotbar_row_1_slot_7, %IniFile%, Hotbar Row 1, slot 7
		Iniread, Hotbar_row_1_slot_8, %IniFile%, Hotbar Row 1, slot 8
		Iniread, Hotbar_row_1_slot_9, %IniFile%, Hotbar Row 1, slot 9
		Iniread, Hotbar_row_1_slot_10, %IniFile%, Hotbar Row 1, slot 10
		Iniread, Hotbar_row_1_slot_11, %IniFile%, Hotbar Row 1, slot 11
		Iniread, Hotbar_row_1_slot_12, %IniFile%, Hotbar Row 1, slot 12
		
		iniread, CheckBoxState, %inifile%, % Checkboxs["CB_State"], % Checkboxs["T_CB"]
	}
	
	Hotbar_Row_2:
	{
		Iniread, Hotbar_row_2_slot_1, %IniFile%, Hotbar Row 2, slot 1
		Iniread, Hotbar_row_2_slot_2, %IniFile%, Hotbar Row 2, slot 2
		Iniread, Hotbar_row_2_slot_3, %IniFile%, Hotbar Row 2, slot 3
		Iniread, Hotbar_row_2_slot_4, %IniFile%, Hotbar Row 2, slot 4
		Iniread, Hotbar_row_2_slot_5, %IniFile%, Hotbar Row 2, slot 5
		Iniread, Hotbar_row_2_slot_6, %IniFile%, Hotbar Row 2, slot 6
		Iniread, Hotbar_row_2_slot_7, %IniFile%, Hotbar Row 2, slot 7
		Iniread, Hotbar_row_2_slot_8, %IniFile%, Hotbar Row 2, slot 8
		Iniread, Hotbar_row_2_slot_9, %IniFile%, Hotbar Row 2, slot 9
		Iniread, Hotbar_row_2_slot_10, %IniFile%, Hotbar Row 2, slot 10
		Iniread, Hotbar_row_2_slot_11, %IniFile%, Hotbar Row 2, slot 11
		Iniread, Hotbar_row_2_slot_12, %IniFile%, Hotbar Row 2, slot 12
		
		
	}
	gosub loadforward
	gosub loadbackward
	gosub loadleft
	gosub loadright
	gosub loadsit
	gosub loadFollow
	gosub loadTarget_party_slot_1
	gosub loadTarget_party_slot_2
	gosub loadTarget_party_slot_3
	gosub loadTarget_party_slot_4
	gosub loadTarget_party_slot_5
	gosub loadTarget_party_slot_6
	gosub loadTarget_party_slot_self
	gosub loadHotbar_row_1_slot_1
	gosub loadHotbar_row_1_slot_2
	gosub loadHotbar_row_1_slot_3
	gosub loadHotbar_row_1_slot_4
	gosub loadHotbar_row_1_slot_5
	gosub loadHotbar_row_1_slot_6
	gosub loadHotbar_row_1_slot_7
	gosub loadHotbar_row_1_slot_8
	gosub loadHotbar_row_1_slot_9
	gosub loadHotbar_row_1_slot_10
	gosub loadHotbar_row_1_slot_11
	gosub loadHotbar_row_1_slot_12
	gosub loadHotbar_row_2_slot_1
	gosub loadHotbar_row_2_slot_2
	gosub loadHotbar_row_2_slot_3
	gosub loadHotbar_row_2_slot_4
	gosub loadHotbar_row_2_slot_5
	gosub loadHotbar_row_2_slot_6
	gosub loadHotbar_row_2_slot_7
	gosub loadHotbar_row_2_slot_8
	gosub loadHotbar_row_2_slot_9
	gosub loadHotbar_row_2_slot_10
	gosub loadHotbar_row_2_slot_11
	gosub loadHotbar_row_2_slot_12
}


