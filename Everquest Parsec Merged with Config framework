	!^h::
	{
		if (WinExist("Everquest Alt Hot Bar"))
		{
			WinGetPos, Hotbar_Position_x, Hotbar_Position_y, , ,
			IniWrite, %Hotbar_Position_x%, %IniFile%, Gui Location, Everquest Alt Hot Bar x
			IniWrite, %Hotbar_Position_y%, %IniFile%, Gui Location, Everquest Alt Hot Bar y		
			gui, hot_bar_gui:hide
			return
		}	
		else
		{
			hotbarlocation:
			{
				IniRead, Hotbar_Position_x, %IniFile%, Gui Location, Everquest Alt Hot Bar x
				IniRead, Hotbar_Position_y, %IniFile%, Gui Location, Everquest Alt Hot Bar y
				if (Hotbar_Position_x="")
				{
					Gui, hot_bar_gui:+toolwindow -Caption -sysmenu +LastFound
					Gui, hot_bar_gui:show, h115 w494, %hotbar%
				}
				else
				{
					Gui, hot_bar_gui:+toolwindow -Caption -sysmenu +LastFound
					Gui, hot_bar_gui:show, x%Hotbar_Position_x% y%Hotbar_Position_y% h115 w494, %hotbar%
				}
			}
			WinGetPos, Hotbar_Position_x, Hotbar_Position_y, , , Everquest Alt Hot Bar
			IniWrite, %Hotbar_Position_x%, %IniFile%, Gui Location, Everquest Alt Hot Bar x
			IniWrite, %Hotbar_Position_y%, %IniFile%, Gui Location, Everquest Alt Hot Bar y
			Gui, hot_bar_gui:Color, CADD22
			;WinSet, Transparent, 21, Everquest Alt Hot Bar ; CADD22
			WinSet, TransColor, CADD22 ;%TransparentBar% would make the bar barely visable
			WinSet, AlwaysOnTop, On, Everquest Alt Hot Bar
			
		}
	}
			OnMessage(0x201, "WM_LBUTTONDOWN")
			
			hotbars:
			{
				Row_1:
				{
					Gui, hot_bar_gui:Font, s7 w400, Verdana
					Gui, hot_bar_gui:Add, Button, x4 y0 w38 h38 +Center +Border, R1S1 ;; need to adjust bar spacing to standard hostbar spacing ingame
					Gui, hot_bar_gui:Add, Button, x45 y0 w38 h38 +Center +Border, R1S2
					Gui, hot_bar_gui:Add, Button, x85 y0 w38 h38 +Center +Border, R1S3
					Gui, hot_bar_gui:Add, Button, x125 y0 w38 h38 +Center +Border, R1S4
					Gui, hot_bar_gui:Add, Button, x165 y0 w38 h38 +Center +Border, R1S5
					Gui, hot_bar_gui:Add, Button, x205 y0 w38 h38 +Center +Border, R1S6
					Gui, hot_bar_gui:Add, Button, x245 y0 w38 h38 +Center +Border, R1S7
					Gui, hot_bar_gui:Add, Button, x285 y0 w38 h38 +Center +Border, R1S8
					Gui, hot_bar_gui:Add, Button, x325 y0 w38 h38 +Center +Border, R1S9
					Gui, hot_bar_gui:Add, Button, x365 y0 w38 h38 +Center +Border, R1S10
					Gui, hot_bar_gui:Add, Button, x405 y0 w38 h38 +Center +Border, R1S11
					Gui, hot_bar_gui:Add, Button, x445 y0 w38 h38 +Center +Border, R1S12
				}
				
				Row_2:
				{
					Gui, Font, s7 w400, Verdana
					Gui, hot_bar_gui:Add, Button, x4 y42 w38 h38 +Center +Border, R2S1
					Gui, hot_bar_gui:Add, Button, x45 y42 w38 h38 +Center +Border, R2S2
					Gui, hot_bar_gui:Add, Button, x85 y42 w38 h38 +Center +Border, R2S3
					Gui, hot_bar_gui:Add, Button, x125 y42 w38 h38 +Center +Border, R2S4
					Gui, hot_bar_gui:Add, Button, x165 y42 w38 h38 +Center +Border, R2S5
					Gui, hot_bar_gui:Add, Button, x205 y42 w38 h38 +Center +Border, R2S6
					Gui, hot_bar_gui:Add, Button, x245 y42 w38 h38 +Center +Border, R2S7
					Gui, hot_bar_gui:Add, Button, x285 y42 w38 h38 +Center +Border, R2S8
					Gui, hot_bar_gui:Add, Button, x325 y42 w38 h38 +Center +Border, R2S9
					Gui, hot_bar_gui:Add, Button, x365 y42 w38 h38 +Center +Border, R2S10
					Gui, hot_bar_gui:Add, Button, x405 y42 w38 h38 +Center +Border, R2S11
					Gui, hot_bar_gui:Add, Button, x445 y42 w38 h38 +Center +Border, R2S12
				}
			}
	
			return
		
		return
	
	
	!h::
	{
		toggle_alt_h := !toggle_alt_h
		if toggle_alt_h{
			Gui, hot_bar_gui:Color, CADD22
			WinSet, TransColor, CADD22, Everquest Alt Hot Bar
			IniRead, Hotbar_Position_x, Config.ini, Gui Location, Everquest Alt Hot Bar x
			IniRead, Hotbar_Position_y, Config.ini, Gui Location, Everquest Alt Hot Bar y	
			Gui, hot_bar_gui:+Caption
			return
		}
		else {
			WinSet, TransColor, CADD22, Everquest Alt Hot Bar
			Gui, hot_bar_gui:-Caption
			return
		}
		return
	}	
	
	
